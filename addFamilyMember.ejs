<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>EL7A2Ni Clinic</title>
  <link rel="stylesheet" href="theme.css">
</head>

<body>
  <header>
      <h1>
      <img src="acllogo.png"alt="logo" width="150px" height="80px"> </h1>
      <hq>Welcome to EL7A2Ni Clinic</hq>
  </header>    
<form action="/addfamilymember/651eb9e4db2f09bf12c528ac" method="post" onSubmit="handleSubmit(event)">
  <div class="centered-form">

    <label>
      Name:
      <input type="text" name="Name" required="true" value="<%= formData.Name %>" onChange="handleChange(event)">
    </label>

    <label>
      National ID:
      <input type="text" name="NationalID " required="true" value="<%= formData.NationalID %>" onChange="handleChange(event)">
    </label>

    <label>
        Age:
        <input type="number" name="Age" required="true" value="<%= formData.Age %>" onChange="handleChange(event)">
      </label>

      <label>
        Gender:
        <input type="text" name="Gender" required="true" value="<%= formData.Gender %>" onChange="handleChange(event)">
      </label>

      <label>
        Relation:
        <input type="text" name="Relation" required="true" value="<%= formData.Relation %>" onChange="handleChange(event)">
      </label>

    <button type="submit" id="register">Register</button>

  </div>
</form>
</body>

<script>
  function handleChange(event) {
    console.log("1");
    const { name, value } = event.target;
    console.log("2");
    setFormData({ ...formData, [name]: value });
    console.log("3");
  }

  function handleSubmit(event) {
    console.log("a");
    event.preventDefault();
    console.log("b");
    // Assuming createAdminstrator is a function defined in your client-side code
    createAdminstrator(formData);
    console.log("c");
  }
</script>
</html>